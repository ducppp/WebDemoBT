@{
	ViewData["Title"] = "Home Page";
}

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta name="description" content="">
	<meta name="author" content="ThemeBucket">
	<link rel="shortcut icon" href="images/favicon.png">
	<title>BucketAdmin</title>
	<!--Core CSS -->
	<link href="~/Admin/bs3/css/bootstrap.min.css" rel="stylesheet">
	<link href="~/Admin/js/jquery-ui/jquery-ui-1.10.1.custom.min.css" rel="stylesheet">
	<link href="~/Admin/css/bootstrap-reset.css" rel="stylesheet">
	<link href="~/Admin/font-awesome/css/font-awesome.css" rel="stylesheet">
	<link href="~/Admin/js/jvector-map/jquery-jvectormap-1.2.2.css" rel="stylesheet">
	<link href="~/Admin/css/clndr.css" rel="stylesheet">
	<!--clock css-->
	<link href="js/css3clock/css/style.css" rel="stylesheet">
	<!--Morris Chart CSS -->
	<link rel="~/Admin/stylesheet" href="~/Admin/js/morris-chart/morris.css">
	<!-- Custom styles for this template -->
	<link href="~/Admin/css/style.css" rel="stylesheet">
	<link href="~/Admin/css/style-responsive.css" rel="stylesheet" />
	<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
	<script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
	<![endif]-->
</head>
<body>
	<section id="container">
		<!--header start-->
		@Html.Partial("_Header")
		<!--header end-->
		<!--sidebar start-->
		<aside>
			<div id="sidebar" class="nav-collapse">
				<!-- sidebar menu start-->
				@Html.Partial("_MenuLeft")

				<!-- sidebar menu end-->
			</div>
		</aside>
		<!--sidebar end-->
		<!--main content start-->
		<section id="main-content">
			<section class="wrapper">

				@RenderBody()
			</section>
		</section>
		<!--main content end-->
		<!--right sidebar start-->
		@Html.Partial("_MenuRight")

		<!--right sidebar end-->
	</section>
	<!-- Placed js at the end of the document so the pages load faster -->
	<!--Core js-->
	<script src="~/Admin/js/jquery.js"></script>
	<script src="~/Admin/js/jquery-ui/jquery-ui-1.10.1.custom.min.js"></script>
	<script src="~/Admin/bs3/js/bootstrap.min.js"></script>
	<script src="~/Admin/js/jquery.dcjqaccordion.2.7.js"></script>
	<script src="~/Admin/js/jquery.scrollTo.min.js"></script>
	<script src="~/Admin/js/jQuery-slimScroll-1.3.0/jquery.slimscroll.js"></script>
	<script src="~/Admin/js/jquery.nicescroll.js"></script>
	<!--[if lte IE 8]><script language="javascript" type="text/javascript" src="js/flot-chart/excanvas.min.js"></script><![endif]-->
	<script src="~/Admin/js/skycons/skycons.js"></script>
	<script src="~/Admin/js/jquery.scrollTo/jquery.scrollTo.js"></script>
	<script src="~/Admin///cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
	<script src="js/calendar/clndr.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.5.2/underscore-min.js"></script>
	<script src="~/Admin/js/calendar/moment-2.2.1.js"></script>
	<script src="~/Admin/js/evnt.calendar.init.js"></script>
	<script src="~/Admin/js/jvector-map/jquery-jvectormap-1.2.2.min.js"></script>
	<script src="~/Admin/js/jvector-map/jquery-jvectormap-us-lcc-en.js"></script>
	<script src="~/Admin/js/gauge/gauge.js"></script>
	<!--clock init-->
	<script src="~/Admin/js/css3clock/js/css3clock.js"></script>
	<!--Easy Pie Chart-->
	<script src="~/Admin/js/easypiechart/jquery.easypiechart.js"></script>
	<!--Sparkline Chart-->
	<script src="~/Admin/js/sparkline/jquery.sparkline.js"></script>
	<!--Morris Chart-->
	<script src="~/Admin/js/morris-chart/morris.js"></script>
	<script src="~/Admin/js/morris-chart/raphael-min.js"></script>
	<!--jQuery Flot Chart-->
	<script src="~/Admin/js/flot-chart/jquery.flot.js"></script>
	<script src="~/Admin/js/flot-chart/jquery.flot.tooltip.min.js"></script>
	<script src="~/Admin/js/flot-chart/jquery.flot.resize.js"></script>
	<script src="~/Admin/js/flot-chart/jquery.flot.pie.resize.js"></script>
	<script src="~/Admin/js/flot-chart/jquery.flot.animator.min.js"></script>
	<script src="~/Admin/js/flot-chart/jquery.flot.growraf.js"></script>
@* 	<script src="~/Admin/js/dashboard.js"></script> *@
	<script src="~/Admin/js/jquery.customSelect.min.js"></script>
	<!--common script init for all pages-->
	<script src="~/Admin/js/scripts.js"></script>
	<!--script for this page-->
	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>